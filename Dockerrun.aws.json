{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name":"nginx",
			"image":"tonyhong568/multi-nginx",
			"hostname":"nginx",
			"essential":true,	
			"portMappings": [
			{
				"hostPort": 80,
				"containerPort": 80
			}
			],
			"links": ["client", "server"],
			"memory": 128
		}
	]
}
